{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@infra/aws": [
        "libs/aws/src"
      ],
      "@infra/aws/*": [
        "libs/aws/src/*"
      ],
      "@auth/auth": [
        "libs/auth/src"
      ],
      "@auth/auth/*": [
        "libs/auth/src/*"
      ],
      "@infra/persistence": [
        "libs/persistence/src"
      ],
      "@infra/persistence/*": [
        "libs/persistence/src/*"
      ],
      "@domain/user": [
        "libs/user/src"
      ],
      "@domain/user/*": [
        "libs/user/src/*"
      ],
      "@infra/auth-strategy": [
        "libs/auth-strategy/src"
      ],
      "@infra/auth-strategy/*": [
        "libs/auth-strategy/src/*"
      ],
      "@domain/domain-user": [
        "libs/domain-user/src"
      ],
      "@domain/domain-user/*": [
        "libs/domain-user/src/*"
      ],
      "@persistence/persistence-user": [
        "libs/persistence-user/src"
      ],
      "@persistence/persistence-user/*": [
        "libs/persistence-user/src/*"
      ],
      "@persistence/persistence": [
        "libs/persistence/src"
      ],
      "@persistence/persistence/*": [
        "libs/persistence/src/*"
      ],
      "@domain/domain-manager": [
        "libs/domain-manager/src"
      ],
      "@domain/domain-manager/*": [
        "libs/domain-manager/src/*"
      ],
      "@infra/kakao": [
        "libs/kakao/src"
      ],
      "@infra/kakao/*": [
        "libs/kakao/src/*"
      ],
      "@domain/domain": [
        "libs/domain/src"
      ],
      "@domain/domain/*": [
        "libs/domain/src/*"
      ]
    }
  }
}